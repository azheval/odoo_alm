<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="alm_project.view_project_form_inherit_alm" model="ir.ui.view">
        <field name="name">project.project.form.inherit.alm</field>
        <field name="model">project.project</field>
        <field name="inherit_id" ref="project.edit_project"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='tag_ids']" position="after">
                <field name="configurable_unit_ids" widget="many2many_tags"/>
            </xpath>
        </field>
    </record>
</odoo>